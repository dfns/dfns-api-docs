{
  "compilerOptions": {
    "allowJs": true,
    "target": "es2018",
    "lib": ["dom", "es2018", "esnext.asynciterable"],
    "incremental": false,
    "declaration": true,
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
